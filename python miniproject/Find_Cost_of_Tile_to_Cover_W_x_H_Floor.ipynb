        "costToCover = lambda w,h,ppm: w*h*ppm\n",
        "print(costToCover(50,100,0.5))\n",
        "\n",
        "\n",
        "def find_max_comb(seq):\n",
        "    temp = 0\n",
        "    for i,n in enumerate(seq):\n",
        "        for v in seq[i+1:]:\n",
        "            temp = max(temp,n+v)\n",
        "    return temp\n",
        "\n",
        "print(find_max_comb([1,7,3,1,3,5,4]))\n",
